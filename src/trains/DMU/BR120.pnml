// unit definition
item (FEAT_TRAINS, item_BR120, 1200) {
    property {
        // common properties
        name:                           string(STR_NAME_BR120);
        climates_available:             ALL_CLIMATES;
        introduction_date:              date(1958,01,01); 
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life: 					40;
        retire_early: 					-10;							
        reliability_decay: 				20;
        
		refittable_cargo_classes:        bitmask(CC_PASSENGERS);
        // non_refittable_cargo_classes:    bitmask();
        cargo_allow_refit:               [PASS, TOUR];
        // cargo_disallow_refit:            [LVST];
        misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
        cargo_capacity:                 1;
        // for MUs, loading speed is multiplied by number of carriages * 0.75
        loading_speed:					34; // Standard Class
        cost_factor: 					108;
        running_cost_base:			    RUNNING_COST_STEAM;					
        running_cost_factor: 			90;

        //specific properties
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          70 mph;
        refit_cost:                     0;
        track_type:                     RAIL;
        power:                          600 hp;
        weight:                         105 ton;
        engine_class:                   ENGINE_CLASS_DIESEL;
        dual_headed:                    0;
        air_drag_coefficient:           0.10;
        tractive_effort_coefficient:    0.06;
        length:                         4;
    }

    graphics {
        // graphics
        default:                        sw_livery_BR120;
        visual_effect_and_powered:      sw_FX_BR120_Diesel;
        purchase:                       sw_BR120_purchase_sprite;
        sound_effect:                   sw_sound_diesel_25;

        // internal code
        articulated_part:               sw_articulated_BR120;
        purchase_cargo_capacity:        return ( 18 * param_pax); // divide by number of articulated parts and floor
        cargo_capacity:                 sw_capacity_BR120;
        cost_factor:                    176;
        running_cost_factor:            3764;
        cargo_subtype_text:             sw_BR120_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;

        // purchase menu text
        additional_text:                return( string(str_purchase_loco_with_liveries,
                                                    string(str_purchase_type_dmu),
                                                    string(str_usage_pax),
                                                    string(str_BR120_liveries)));
    }
}

