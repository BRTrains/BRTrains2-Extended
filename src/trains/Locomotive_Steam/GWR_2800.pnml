// unit definition
item (FEAT_TRAINS, item_GWR_2800, 280) {
    property {
        // common properties
        name:                           string(STR_NAME_GWR_2800);
        climates_available:             ALL_CLIMATES;
        introduction_date:              date(1903,01,01); 
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life: 					40;
        retire_early: 					-10;							
        reliability_decay: 				20;
        
		// refittable_cargo_classes:        bitmask(CC_PASSENGERS);
        // non_refittable_cargo_classes:    bitmask();
        cargo_allow_refit:               [PASS, GOOD];
        // cargo_disallow_refit:            [LVST];
        misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
        cargo_capacity:                 1;
        cost_factor: 					76;
        running_cost_base:			    RUNNING_COST_STEAM;					
        running_cost_factor: 			4939;

        //specific properties
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          40 mph;
        refit_cost:                     0;
        track_type:                     RAIL;
        power:                          2210 hp;
        weight:                         117 ton;
        engine_class:                   ENGINE_CLASS_STEAM;
        dual_headed:                    0;
        air_drag_coefficient:           0.08;
        tractive_effort_coefficient:    0.14;
        length:                         8;
    }

    graphics {
        // graphics
        default:                        sw_graphics_GWR_2800;
        visual_effect_and_powered:      sw_FX_GWR_2800_Steam;
        purchase:                       sw_spritestack_GWR_2800_purchase; // For dual mode units
        sound_effect:                   sw_sound_steam_generic;

        // internal code
        length:                         sw_length_GWR_2800;
        articulated_part:               sw_articulated_GWR_2800;
        speed:                          sw_speed_GWR_2800() * 1609 / 1000;  
        purchase_cargo_capacity:        0;
        cargo_capacity:                 return 0;
        cost_factor:                    76;
        running_cost_factor:            4939;
        cargo_subtype_text:             sw_GWR_2800_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;
        // purchase menu text
        additional_text:                return( string(str_purchase_loco_with_liveries,
                                                    string(str_purchase_type_steam),
                                                    string(str_usage_heavy_freight),
                                                    string(str_GWR_2800_liveries)));
    }
}

