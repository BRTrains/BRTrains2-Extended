        dual_headed:                    0;
        air_drag_coefficient:           0.05;
        tractive_effort_coefficient:    0.12;
        length:                         8;
    }

    graphics {
        // graphics
        default:                        sw_graphics_SECR_N1;
        visual_effect_and_powered:      sw_FX_SECR_N1_Steam;
        purchase:                       sw_spritestack_SECR_N1_purchase;
        sound_effect:                   sw_sound_steam_generic;

        // internal code
        length:                         sw_length_SECR_N;
        articulated_part:               sw_articulated_SECR_N1;
        purchase_cargo_capacity:        0;
        cargo_capacity:                 return 0;
        cost_factor:                    72;
        running_cost_factor:            4793;
        cargo_subtype_text:             sw_SECR_N1_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;

        // purchase menu text
        additional_text:                return( string(str_purchase_loco_with_liveries,
                                                    string(str_purchase_type_steam),
                                                    string(str_usage_mixed_traffic),
                                                    string(str_SECR_N_liveries)));
    }
}

