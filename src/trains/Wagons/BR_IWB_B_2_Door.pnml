// livery name refit switch
switch(FEAT_TRAINS, SELF, sw_BR_IWB_B_2_Door_cargo_subtype_text, cargo_subtype){
	0:	return string(STR_LIVERY_SHORT, string(str_col_Grey));
    1:  return string(STR_LIVERY_SHORT, string(str_col_Bauxite));
    2:  return string(STR_LIVERY_SHORT, string(str_col_Blue));
    3:  return string(STR_LIVERY_SHORT, string(str_col_Silver));
    4:  return string(STR_LIVERY_BRANDED, string(str_col_Blue), string(str_var_Cargowaggon));
    5:  return string(STR_LIVERY_BRANDED, string(str_col_Blue), string(str_col_Blue));
    6:  return string(str_VTG);
    7:  return string(str_RailAdventure);
       return CB_RESULT_NO_TEXT; }

// graphics switch
switch(FEAT_TRAINS, SELF, sw_graphics_BR_IWB_B_2_Door, position_in_articulated_veh % 3) {
	1: return sw_livery_BR_IWB_B_2_Door;
	    spriteset_empty; }

// articulation switch
switch(FEAT_TRAINS, SELF, sw_articulated_BR_IWB_B_2_Door, extra_callback_info1) {
	1..2: return item_BR_IWB_B_2_Door;
		return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}


