        refit_cost:                     0;
        track_type:                     RAIL;
        weight:                         26 ton;
        dual_headed:                    0;
        length:                         4;
    }

    graphics {
        // graphics
        default:                        sw_graphics_BR_IWB_B_2_Door;
        purchase:                       spriteset_BR_IWB_B_2_Door_Blue_Cargowaggon_purchase;

        // internal code
        articulated_part:               sw_articulated_BR_IWB_B_2_Door;
        cost_factor:                    952;
        running_cost_factor:            1048;
        cargo_subtype_text:             sw_BR_IWB_B_2_Door_cargo_subtype_text; 
